package POJO;

public class Location {

	private static String lat;
	private static String lng;
	
	
	public static String getLat() {
		return lat;
	}
	public static void setLat(String lat) {
		Location.lat = lat;
	}
	public static String getLng() {
		return lng;
	}
	public static void setLng(String lng) {
		Location.lng = lng;
	}
	
}
