package POJO;

public class Location {

	private static int lat;
	private static int lng;
	
	
	public static int getLat() {
		return lat;
	}
	public static void setLat(int lat) {
		Location.lat = lat;
	}
	public static int getLng() {
		return lng;
	}
	public static void setLng(int lng) {
		Location.lng = lng;
	}
	
	
	
	
	
}
